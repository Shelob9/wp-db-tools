{
    "name": "inpsyde/wp-db-tools",
    "description": "Provides an engine-independent, simplified interface to MySQL databases.",
    "license": "MIT",
    "authors": [
        {
            "name": "Inpsyde GmbH",
            "role": "Company",
            "homepage": "http://inpsyde.com"
        },
        {
            "name": "David Naber",
            "email": "kontakt@dnaber.de",
            "homepage": "https://dnaber.de/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "symfony/yaml": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "WpDbTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WpDbTools\\Test\\": "tests/src/"
        }
    },
    "require-dev": {
        "inpsyde/monkery-test-case": "^3.0",
        "inpsyde/wp-tests-starter": "^1.0",
        "inpsyde/wordpress-dev": "^4.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/inpsyde/wp-db-types.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/inpsyde/wp-prepared-statements-converter.git"
        }
    ]
}
